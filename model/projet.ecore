<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="modèle" nsURI="http://m2sdl.modèle" nsPrefix="modèle">
  <eAnnotations source="http://www.eclipse.org/OCL/Import">
    <details key="ecore" value="http://www.eclipse.org/emf/2002/Ecore"/>
  </eAnnotations>
  <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
    <details key="invocationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EEnum" name="Fonction">
    <eLiterals name="Analyst"/>
    <eLiterals name="Designer" value="1"/>
    <eLiterals name="Programmeur" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Entreprise" eSuperTypes="#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="projet" upperBound="-1"
        eType="#//Projet" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="personnel" upperBound="-1"
        eType="#//Membre" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="poste" upperBound="-1"
        eType="#//Role" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Element" abstract="true" interface="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nom" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Projet" eSuperTypes="#//Element">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="roleDansProjetUnique auMoinsDeuxRolesDifferents"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="roleDansProjetUnique" value="&#xA;&#x9;&#x9;&#x9;self.membres->notEmpty() implies self.membres->isUnique(nom)"/>
      <details key="auMoinsDeuxRolesDifferents" value="&#xA;&#x9;&#x9;&#x9;let f : Role = self.roles->first() in&#xA;&#x9;&#x9;&#x9;self.roles->notEmpty() implies self.roles->collect(r|r.fonction &lt;> f.fonction)->size()>=1"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="debut" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="fin" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="membres" lowerBound="2"
        upperBound="-1" eType="#//Membre" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="chef" lowerBound="1" eType="#//Membre"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="roles" upperBound="-1"
        eType="#//Role" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Membre" eSuperTypes="#//Element">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="membreDifferentDeChef"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="membreDifferentDeChef" value="&#xA;&#x9;&#x9;&#x9;self.projets->notEmpty() implies self.projets->forAll(p: Projet | p.chef.nom&lt;>self.nom)"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="projets" upperBound="-1"
        eType="#//Projet"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="projetsGeres" upperBound="-1"
        eType="#//Projet"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="role" upperBound="-1" eType="#//Role"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Role">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="fonction" eType="#//Fonction"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="membre" eType="#//Membre"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="projet" eType="#//Projet"/>
  </eClassifiers>
</ecore:EPackage>
